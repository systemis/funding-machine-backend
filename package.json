  "name": "hamstermachine",
  "version": "0.0.1",
  "description": "Hamstermachine package",
  "author": "khang@cavies.xyz",
  "private": true,
  "type": "commonjs",
  "license": "MIT",
  "redisMemoryServer": {
    "debug": "1"
