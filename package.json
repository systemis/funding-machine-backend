  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "command": "npm run build && node dist/src/command.js",
    "start:worker-dev": "nest start -c nest-cli.worker.json --watch",
    "start:worker-prod": "node dist/src/worker.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "./scripts/test.sh",
    "test:e2e": "./scripts/test-e2e.sh --ci",
    "test:e2e-dev": "./scripts/test-e2e.sh --dev",
    "prepare": "husky install"
  },
